prefix=@REPROC_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@
libdir=${prefix}/@CMAKE_INSTALL_LIBDIR@

Name: reproc++
Description: "Cross-platform C++11 library that simplifies working with external CLI applications from C++"
URL: https://github.com/DaanDeMeyer/reproc
Version: @PROJECT_VERSION@
Requires.private: reproc = @PROJECT_VERSION@
Cflags: -I${includedir}
Libs: -L${libdir} -lreproc++

